{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "firstName",
        "lastName",
        "username",
        "password",
        "email",
        "isAdmin"
    ],
    "properties": {
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "Test"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "User"
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "testUser"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "test1234"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "testuser@testemail.com"
            ]
        },
        "isAdmin": {
            "type": "boolean",
            "default": false,
            "title": "The isAdmin Schema",
            "examples": [
                false
            ]
        }
    },
    "examples": [{
        "firstName": "Test",
        "lastName": "User",
        "username": "testUser",
        "password": "test1234",
        "email": "testuser@testemail.com",
        "isAdmin": false
    }]
}